<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%h %l %u [%t] "%r" %s %b "%i{Referer}" "%i{User-Agent}" [%i{B3}%i{traceparent}]</pattern>
    </encoder>
  </appender>

  <appender-ref ref="STDOUT" />

</configuration>